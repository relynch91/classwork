<h1>Sign Up</h1>
<form action="<%= users_url %>"method="Post">
    <input type="hidden" 
        name = "authenticity_token" 
        value="<%= form_authenticity_token%>"/>
        
    <label>Username
        <input name="user[username]" value="<%=@user.username%>"/>
    </label>
    <label>Password
        <input type="password" name="user[password]"/>
    </label>
    <button>Sign Up</button>
</form>